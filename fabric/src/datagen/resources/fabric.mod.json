{
    "schemaVersion": 1,
    "id": "${mod_id}_datagen",
    "version": "${mod_version}+${minecraft_version}",
    "name": "${mod_name} Datagen",
    "description": "${mod_description}",
    "authors": [
        "${mod_author}"
    ],
    "contributors": [
        "${fabric_mod_contributors}"
    ],
    "contact": {
        "homepage": "${homepage}",
        "issues": "https://github.com/${sources}/issues",
        "sources": "https://github.com/${sources}"
    },
    "license": "${mod_license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "fabric-datagen": [
            "net.modgarden.barricade.datagen.BarricadeDataGen"
        ]
    },
    "depends": {
        "fabricloader": "${fabric_loader_range}",
        "fabric-api": "*",
        "minecraft": "${fabric_minecraft_version_range}",
        "java": ">=${java_version}"
    }
}